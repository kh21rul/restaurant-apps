{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prefer-arrow-callback": "off",
        "func-names": "off",
        "indent": "off",
        "no-console": "off",
        "eol-last": "off",
        "import/no-extraneous-dependencies": "off",
        "no-underscore-dangle": "off",
        "operator-linebreak": "off",
        "no-unused-vars": "off",
        "class-methods-use-this": "off",
        "no-undef": "off",
        "no-restricted-globals": "off",
        "prefer-const": "off",
        "no-return-assign": 0,
        "no-new": 0,
        "comma-dangle": 0,
        "semi": 0,
        "linebreak-style": 0
    }
}